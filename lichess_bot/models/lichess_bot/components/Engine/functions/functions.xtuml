within lichess_bot::components::Engine is

	package functions is
		@dialect("oal");
		@function_num(1);
		function resignAll() is
			@noparse
			select many games from instances of Game;
			for each game in games
				game.resign();
			end for;
			@endnoparse
		end function;

	end package;

end;
